#!/usr/bin/make -f

# This file was automatically generated by stdeb 0.6.0+git at
# Tue, 02 Sep 2014 23:17:55 +0000


override_dh_auto_configure:
	QUILT_PATCHES=debian/patches quilt push -a || ( RET=$$?; test 2 -eq $$RET )
	dh_auto_configure

override_dh_auto_clean:
	quilt pop -a || ( RET=$$?; test 2 -eq $$RET )
	rm -rf .pc/
	dh_auto_clean

%:
	dh $@ --with python2 --buildsystem=python_distutils


